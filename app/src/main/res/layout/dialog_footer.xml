<?xml version="1.0" encoding="utf-8"?>
<com.qsmaxmin.qsbase.common.widget.percentlayout.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp">

    <com.qsmaxmin.qsbase.common.widget.percentlayout.PercentRelativeLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        app:layout_widthPercent="50%w">

        <TextView
            android:id="@+id/cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_button_dialog"
            android:text="取消"
            android:textSize="16sp" />
    </com.qsmaxmin.qsbase.common.widget.percentlayout.PercentRelativeLayout>

    <com.qsmaxmin.qsbase.common.widget.percentlayout.PercentRelativeLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        app:layout_widthPercent="50%w">

        <TextView
            android:id="@+id/confirm"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_button_dialog"
            android:text="确定"
            android:textSize="16sp" />
    </com.qsmaxmin.qsbase.common.widget.percentlayout.PercentRelativeLayout>
</com.qsmaxmin.qsbase.common.widget.percentlayout.PercentLinearLayout>

