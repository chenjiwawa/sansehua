<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <include layout="@layout/dialog_header" />

    <RelativeLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/shape_rect_lightgray_2dp_stroke">

        <fragment
            android:id="@+id/labels"
            android:name="com.zl.dappore.voiceroom.fragment.roomsetting.StringGridFragment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </RelativeLayout>

    <include layout="@layout/dialog_footer" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="110dp"
        android:layout_height="55dp"
        android:layout_centerHorizontal="true"
        android:scaleType="fitXY"
        android:src="@android:drawable/ic_delete"
        android:visibility="gone" />

</LinearLayout>

